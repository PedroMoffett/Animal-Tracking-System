<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="200.0" prefWidth="320.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.MainMenuController">
   <children>
      <VBox alignment="CENTER" prefHeight="211.0" prefWidth="320.0" spacing="20.0">
        <padding>
          <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
        </padding>
        <Button maxWidth="92.0" onAction="#onActionCreateAnimal" prefHeight="25.0" prefWidth="87.0" text="CREATE">
            <font>
               <Font size="14.0" />
            </font>
         </Button>
         <Button maxWidth="92.0" mnemonicParsing="false" onAction="#onActionDisplayAnimal" text="DISPLAY">
            <font>
               <Font size="14.0" />
            </font>
         </Button>
         <Button maxWidth="92.0" mnemonicParsing="false" onAction="#onActionExit" text="EXIT">
            <font>
               <Font size="14.0" />
            </font>
         </Button>
      </VBox>
   </children>
</AnchorPane>
